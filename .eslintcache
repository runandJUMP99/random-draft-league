ons.filter(selection => !selection.isSelected);\r\n    \r\n    useEffect(() => {\r\n        dispatch(getSelections(token));\r\n    }, [dispatch]);\r\n\r\n    selectedSelections.sort((a, b) => {\r\n        return a.name.localeCompare(b.name, 'en', {'sensitivity': 'base'});\r\n    });\r\n\r\n    notSelectedSelections.sort((a, b) => {\r\n        return a.name.localeCompare(b.name, 'en', {'sensitivity': 'base'});\r\n    });\r\n\r\n    selections = notSelectedSelections.concat(selectedSelections);\r\n    \r\n    return (\r\n        <div className={classes.Selections}>\r\n            {selections.length === 0 ? <Spinner /> : selections.map(selection => (\r\n                <div className={classes.Selection} key={selection.id} onClick={() => handleSelection(selection.id)}>\r\n                    <Selection \r\n                        selectionData={selection} \r\n                        showModal={showModal}\r\n                    />\r\n                </div>\r\n            ))}\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default Selections;","C:\\Users\\steve\\Desktop\\webDev\\random-draft-league\\src\\components\\Selections\\Selection\\Selection.jsx",[],"C:\\Users\\steve\\Desktop\\webDev\\random-draft-league\\src\\components\\Chart\\Players\\Players.jsx",[],"C:\\Users\\steve\\Desktop\\webDev\\random-draft-league\\src\\components\\Chart\\Rounds\\Rounds.jsx",[],"C:\\Users\\steve\\Desktop\\webDev\\random-draft-league\\src\\components\\Chart\\Rounds\\Round\\Round.jsx",["213"],"import React from \"react\";\r\n\r\nimport RoundSummary from \"./RoundSummary/RoundSummary\";\r\nimport Selection from \"../../../Selections/Selection/Selection\";\r\n\r\nimport classes from \"./Round.module.css\";\r\n\r\nconst Round = ({handleSelection, round, selections, setModalContent, setShowModal}) => {\r\n    function handleClick() {\r\n        setModalContent(<RoundSummary round={round} selections={selections} />);\r\n        setShowModal(true);\r\n    }\r\n  \r\n    return (\r\n        <div c